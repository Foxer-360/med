{
  "name": "Hero",
  "data": {
    "title": "Sample Title",
    "text": "Sample Text",
    "placeholder": "Search..",
    "displaySearch": false,
    "displayOverlay": false,
    "TitleColor": "blue",
    "textColor": "gray",
    "image": {},
    "doctorSearchResults": [{ "name": "", "speciality": "", "clinic": "", "active": false }],
    "blogSearchResults": [{ "title": "", "perex": "", "image": {} }]
  },
  "form": {
    "schema": {
      "type": "object",

      "properties": {
        "displayOverlay": {
          "type": "boolean",
          "title": "Display Overlay?"
        },
        "title": {
          "type": "text",
          "title": "Title"
        },
        "titleColor": {
          "type": "enum",
          "title": "Title Color",
          "options": {
            "blue": "blue",
            "white": "white"
          }
        },
        "text": {
          "type": "text",
          "title": "Text"
        },
        "textColor": {
          "type": "enum",
          "title": "Text Color",
          "options": {
            "gray": "gray",
            "white": "white"
          }
        },
        "placeholder": {
          "type": "text",
          "title": "placeholder"
        },
        "displaySearch": {
          "type": "boolean",
          "title": "Display Search Bar?"
        },
        "image": {
          "type": "image"
        },

        "doctorSearchResults": {
          "type": "array",
          "title": "Doctor Search Results",
          "items": {
            "properties": {
              "name": {
                "type": "text",
                "title": "Doctor result name"
              },
              "speciality": {
                "type": "text",
                "title": "Doctor result Speciality"
              },
              "clinic": {
                "type": "text",
                "title": "Doctor result clinic"
              },
              "active": {
                "type": "boolean",
                "title": "Doctor result active"
              },
              "workingHours": {
                "type": "text",
                "title": "Working hours"
              },
              "link": {
                "type": "urlAutocomplete",
                "title": "Link URL"
              }
            }
          }
        },

        "blogSearchResults": {
          "type": "array",
          "title": "Doctor Search Results",
          "items": {
            "properties": {
              "title": {
                "type": "text",
                "title": "Blog result title "
              },
              "perex": {
                "type": "text",
                "title": "Blog results perex"
              },
              "image": {
                "type": "image"
              },
              "link": {
                "type": "urlAutocomplete",
                "title": "Link URL"
              }
            }
          }
        }
      }
    }
  }
}
